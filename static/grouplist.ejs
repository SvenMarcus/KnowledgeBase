<div style="position: relative; top: 80px;">
    <% for(const group of groups) { %>
    <a href="/groups/<%= group.id %>" class="title"><%= group.topicName %></a>
    <div class="box">
        <p class="subtitle">Mitglieder</p>
        <div class="columns is-mobile">
            <% for(const member of group.members ) { %>
            <div class="column is-3">
                <img src="/img/user-solid.svg"
                    style="min-width:30px; width:15%; margin-left:auto; margin-right:auto; display:block;">
                <p style="text-align: center;"><%= member.fullName() %></p>
            </div>
            <% } %>
        </div>
    </div>
    <% } %>
</div>